type Foo = { z: integer; }

let topval closure_y : (closure_env, integer) → integer =
  λ (env: closure_env) (z: integer) →
  (from_closure_env env).0 + z 
let scope SubFoo1
  (SubFoo1_in: SubFoo1_in {x_in: integer})
  : SubFoo1 {
      x: integer;
      y: ((closure_env, integer) → integer * closure_env)
