  decl y : decimal;
  y = 1000.;
  return x * y

let glob5 = glob5_init ()

let glob2 = A {"y": glob1 >= 30., "z": 123. * 17.}

let S2 (S2_in: S2_in) =
  decl a1 : decimal;
  decl a2 : option decimal;
  decl a3 : option decimal;
  switch handle_exceptions []:
  | ENone _ →
    if true:
      a3 = ESome glob3 ¤44.00 + 100.
      else:
        a3 = ENone ()
  | ESome x →
    a3 = ESome x;
  switch handle_exceptions [a3]:
  | ENone _ →
    if false:
      a2 = ENone ()
      else:
        a2 = ENone ()
  | ESome x →
    a2 = ESome x;
  switch a2:
  | ENone _ →
    fatal NoValue
  | ESome arg →
    a1 = arg;
  decl a : decimal;
  a = a1;
  return S2 {"a": a}

