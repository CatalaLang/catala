type Assert__1 = { assert: integer; }

#[test]
type Assert_in__1 = {  }

#[test]
type Assert__2 = { assert: bool; }

let topval closure_assert__1 : (closure_env, integer) → integer =
  λ (env: closure_env) (x: integer) →
  let env__1 : () = from_closure_env env in
  
  ((x + 1))

#[test]
let scope assert
  (assert_in: #[test] Assert_in__1)
  : #[test] Assert__2 {assert: bool}
  =
  let set assert__2 : bool =
    match
      (match
         (ESome
            (let assert__3 : list of integer =
               map (let assert__4 : (closure_env, integer) → integer =
                      closure_assert__1
                    in
                    (assert__4, to_closure_env ()))
                 [0]
             in
             false, <conflicts:23.3-59>))
       with
       | ENone → ESome (false, <conflicts:19.10-16>)
       | ESome x → ESome x)
    with
    | ENone → error NoValue
    | ESome arg → arg.0
  in
  return { Assert__2 assert = assert__2; }

let topval assert__1 : integer → integer =
  λ (assert__2: integer) →
  assert__2

let topval closure_assert :
  (Assert_in {assert: integer}, Assert__1 {assert: integer}) =
  ({ Assert_in assert = 99; }, { Assert__1 assert = 199; })

```


